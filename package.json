{
	"name": "react-chat-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"vite --open --host --mode DEV\" \"tsc -w\"",
		"dev:local": "concurrently \"vite --open --host --mode LOCALDEV\" \"tsc -w\"",
		"build": "tsc && vite build --mode PROD",
		"preview": "vite preview",
		"test:unit": "jest --config=./test/config/jest.config.ts",
		"test:ui": "npx chromatic --project-token=b8feaa236f57 --build-script-name=storybook:build",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,cjs,mjs}\"",
		"prepare": "husky install",
		"create:shared": "plop componentWithoutModel -- --layer shared",
		"create:widget": "plop componentWithoutModel -- --layer widgets",
		"create:page": "plop page --",
		"create:entitie": "plop componentWithModel -- --layer entities",
		"create:feature": "plop componentWithModel -- --layer features"
	},
